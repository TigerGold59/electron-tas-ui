{
  "name": "electron-tas-ui",
  "productName": "Electron TAS UI",
  "version": "0.0.1",
  "description": "Interface for generating hamhub7/nx-TAS scripts and uploading them.",
  "main": "src/main.js",
  "scripts": {
    "start": "tsc; electron .",
    "make": "electron-builder build; electron-builder build -w; electron-builder build -l",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "TigerGold59",
    "email": "bruh@gmail.com"
  },
  "build": {
    "appId": "tiger.electron_tas_ui",
    "mac": {
      "icon": "build/icon.png"
    },
    "dmg": {
      "icon": "build/icon.png"
    },
    "win": {
      "icon": "build/icon.png",
      "target": "portable"
    },
    "linux": {
      "icon": "build/icon.png"
    }
  },
  "license": "MIT",
  "config": {},
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parser": "@typescript-eslint/parser",
    "rules": {
      "@typescript-eslint/camelcase": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "electron": "8.2.5",
    "electron-builder": "^22.6.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/electron-packager": "^14.0.0",
    "@types/ftp": "^0.3.31",
    "@types/jquery": "^3.3.38",
    "@types/lodash": "^4.14.151",
    "@types/ping": "^0.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "ftp": "^0.3.10",
    "ping": "^0.2.3"
  }
}
